{
  "name": "10up/tenup-theme",
  "type": "wordpress-theme",
  "authors": [
    {
      "name": "10up",
      "email": "info@10up.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "10up/wp-framework": "~1.2.0"
  },
  "require-dev": {
    "10up/phpcs-composer": "dev-trunk"
  },
  "scripts": {
    "lint": "phpcs .",
    "lint-fix": "phpcbf ."
  },
  "autoload": {
    "psr-4": {
      "TenupBlockTheme\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
